	<parameter name="ParameterUserId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT 
    u.nome_completo, 
    l.latitude, 
    l.longitude,  
    s.criado_em
FROM 
    localizacao l 
JOIN 
    sessao s
    ON 
    l.sessao_id_1 = s.id
JOIN 
    usuario u
ON 
    l.usuario_id_2 = u.id
 WHERE u.id = $P{ParameterUserId}]]>
	</queryString>

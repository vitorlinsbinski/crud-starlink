	<parameter name="ParameterUserId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
    u.nome_completo AS nome_do_usuario,
    c.nome_usuario AS nome_de_conta,
    c.senha AS senha_da_conta,
    c.criado_em AS data_de_criacao_da_conta,
    c.atualizado_em AS data_de_atualizacao_da_conta,
    s.tipo_dispositivo,
    s.sistema_operacional,
    s.navegador_web,
    s.endereco_ip,
    pc.idioma_preferido,
    pc.configuracoes_privacidade,
    pc.preferencias_notificacao
FROM
    sessao s
JOIN
    usuario u ON u.id = s.usuario_id
JOIN
    conta c ON c.usuario_id = u.id
LEFT JOIN
    preferencias_configuracao pc ON pc.conta_id = c.id
 WHERE 
 u.id =  $P{ParameterUserId}]]>
	</queryString>

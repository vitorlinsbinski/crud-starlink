	<parameter name="ParameterUserId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT 
    u.id AS usuario_id,
    u.nome_completo,
    u.email,
    u.telefone,
    r.rede_social,
    r.nome_de_usuario_rede_social,
    ac.status_conexao,
    u1.nome_completo AS amigo_nome_completo,
    u1.email AS amigo_email,
    u1.telefone AS amigo_telefone
FROM 
    usuario u
JOIN 
    rede_social_conectada r ON u.id = r.usuario_id
JOIN 
    amizade_conexao ac ON u.id = ac.usuario_id_1 OR u.id = ac.usuario_id_2
JOIN 
    usuario u1 ON (u1.id = ac.usuario_id_1 AND u1.id != u.id) OR (u1.id = ac.usuario_id_2 AND u1.id != u.id)
WHERE
u.id = $P{ParameterUserId}
ORDER BY 
    u.id, r.rede_social, ac.status_conexao]]>
	</queryString>
